{
  "name": "world-cup-stats",
  "version": "1.0.0",
  "description": "Scraper for stats of world cup qatar 2022",
  "main": "./src/js/server.ts",
  "config": {
    "railway": "./node_modules/@railway/cli/bin/railway",
    "nodemon": "./node_modules/nodemon/bin/nodemon.js"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "login": "$npm_package_config_railway login",
    "logout": "$npm_package_config_railway logout",
    "build": "tsc",
    "dev": "$npm_package_config_railway run $npm_package_config_nodemon ./src/js/server.ts",
    "initialize-db": "$npm_package_config_railway run ./scripts/initialize-db.sh"
  },
  "repository": {
    "type": "",
    "url": "yes"
  },
  "author": "Olivier Bitter",
  "license": "ISC",
  "devDependencies": {
    "@railway/cli": "^3.0.12",
    "@types/express": "^4.17.17",
    "@types/express-handlebars": "^6.0.0",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.18.2",
    "mongodb": "^5.1.0",
